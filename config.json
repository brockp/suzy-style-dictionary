{
	"source": ["src/input/transformed-tokens.json"],
	"platforms": {
		"css-variables": {
			"transformGroup": "css",
			"buildPath": "build/css/",
			"files": [
				{
					"destination": "_variables.css",
					"format": "css/variables",
					"options": {
						"showHeader": true,
						"outputReferences": true
					}
				}
			]
		},
		"scss-variables": {
			"transformGroup": "scss",
			"buildPath": "build/css/",
			"files": [
				{
					"destination": "_variables.scss",
					"format": "scss/variables",
					"options": {
						"showHeader": false,
						"outputReferences": true
					}
				}
			]
		},
		"less-variables": {
			"transformGroup": "web",
			"buildPath": "build/css/",
			"files": [
				{
					"destination": "_variables.less",
					"format": "less/variables"
				}
			]
		},
		"json": {
			"transformGroup": "js",
			"buildPath": "build/json/",
			"files": [
				{
					"destination": "_variables.json",
					"format": "json",
					"options": {
						"showFileHeader": false,
						"outputReferences": true
					}
				}
			]
		},
		"js-module": {
			"transformGroup": "js",
			"buildPath": "build/js/",
			"files": [
				{
					"destination": "_variables-module.js",
					"format": "javascript/module",
					"options": {
						"showHeader": false,
						"outputReferences": true
					}
				}
			]
		},
		"js-object": {
			"transformGroup": "js",
			"buildPath": "build/js/",
			"files": [
				{
					"destination": "_variables-object.js",
					"format": "javascript/object",
					"options": {
						"showHeader": false,
						"outputReferences": true
					}
				}
			]
		},
		"js-es6": {
			"transformGroup": "js",
			"buildPath": "build/js/",
			"files": [
				{
					"destination": "_variables-es6.js",
					"format": "javascript/es6",
					"options": {
						"showHeader": false,
						"outputReferences": true
					}
				}
			]
		},
		"ios": {
			"transformGroup": "ios",
			"buildPath": "build/ios/Classes/Generated/",
			"prefix": "Suzy",
			"files": [
				{
					"destination": "StyleDictionary.plist",
					"format": "ios/plist",
					"options": {
						"showHeader": false,
						"outputReferences": true
					}
				},
				{
					"destination": "StyleDictionarySize.h",
					"format": "ios/static.h",
					"type": "float",
					"className": "StyleDictionarySize",
					"filter": {
						"attributes": {
							"category": "size"
						}
					},
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "StyleDictionarySize.m",
					"format": "ios/static.m",
					"type": "float",
					"className": "StyleDictionarySize",
					"filter": {
						"attributes": {
							"category": "size"
						}
					},
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "StyleDictionaryIcons.h",
					"format": "ios/strings.h",
					"className": "StyleDictionaryIcons",
					"filter": {
						"attributes": {
							"category": "content",
							"type": "icon"
						}
					},
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "StyleDictionaryIcons.m",
					"format": "ios/strings.m",
					"className": "StyleDictionaryIcons",
					"filter": {
						"attributes": {
							"category": "content",
							"type": "icon"
						}
					},
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "StyleDictionaryColor.h",
					"format": "ios/colors.h",
					"className": "StyleDictionaryColor",
					"type": "StyleDictionaryColorName",
					"filter": {
						"attributes": {
							"category": "color"
						}
					},
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "StyleDictionaryColor.m",
					"format": "ios/colors.m",
					"className": "StyleDictionaryColor",
					"type": "StyleDictionaryColorName",
					"filter": {
						"attributes": {
							"category": "color"
						}
					},
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "StyleDictionaryProperties.h",
					"format": "ios/singleton.h",
					"className": "StyleDictionaryProperties",
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "StyleDictionaryProperties.m",
					"format": "ios/singleton.m",
					"className": "StyleDictionaryProperties",
					"options": {
						"showHeader": false
					}
				}
			]
		},
		"android": {
			"transformGroup": "android",
			"buildPath": "build/android/styledictionary/src/main/res/values/",
			"files": [
				{
					"destination": "style_dictionary_colors.xml",
					"format": "android/colors",
					"options": {
						"showHeader": false,
						"outputReferences": true
					}
				},
				{
					"destination": "style_dictionary_font_dimens.xml",
					"format": "android/fontDimens",
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "style_dictionary_dimens.xml",
					"format": "android/dimens",
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "style_dictionary_integers.xml",
					"format": "android/integers",
					"options": {
						"showHeader": false
					}
				},
				{
					"destination": "style_dictionary_strings.xml",
					"format": "android/strings",
					"options": {
						"showHeader": false
					}
				}
			]
		}
	}
}
